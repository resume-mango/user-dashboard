{
  
    "env": {
      "production": {
        "plugins": [
          ["react-remove-properties", {
            "properties": ["data-test-id"]}]
        ]
      }
    }
  ,
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
